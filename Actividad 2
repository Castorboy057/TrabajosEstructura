class GestorFrutas:
    def principal(self):
        frutas = []
        frutas.append("Fresa")
        frutas.append("Pera")
        frutas.append("Durazno")
        frutas.append("Piña")
        print(frutas)
        frutas.pop(0)
        frutas.pop(1)
        frutas.append("Cereza")
        print(frutas)

if __name__ == "__main__":
    gestor_frutas = GestorFrutas()
    gestor_frutas.principal()



Memoria estática 

from tkinter import simpledialog, Tk

class MemoriaEstatica:
    def main(self):
        calificaciones = []
        for i in range(5):
            calificacion = simpledialog.askinteger("Calificación", f"Captura la calificación {i + 1}:")
            calificaciones.append(calificacion)
        print(calificaciones)

if __name__ == "__main__":
    root = Tk()
    memoria_estatica = MemoriaEstatica()
    memoria_estatica.main()
    root.destroy()



import numpy as np

calificaciones = np.random.randint(0, 101, size=(100, 6))

print("Matriz de Calificaciones:")
print(calificaciones)


alumno = 95
materia = 5
calificacion = calificaciones[alumno - 1, materia - 1] 
print(f"\nCalificación del alumno {alumno} en la materia {materia}: {calificacion}")

