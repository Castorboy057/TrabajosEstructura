


lista = [1, 2, 3, 4, 5, 6, 3, 7, 8, 2]

def detectar_duplicados(lista):
    diccionario = {}
    duplicados = []
    for elemento in lista:
        if elemento in diccionario:
            duplicados.append(elemento)
        else:
            diccionario[elemento] = 1
    return duplicados

print(detectar_duplicados(lista))  # Deber√≠a encontrar los duplicados [3, 2]

